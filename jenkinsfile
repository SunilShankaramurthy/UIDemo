pipeline {
    agent any
    stages {
        stage('Cleaning Stage') {
            steps {
                   sh "mvn clean"
            }
        }
        stage('Testing Stage') {
            steps {
                   sh "mvn test -Denv=nonprod -DBrowser=chrome -Dtestng.dtd.http=true"
            }
        }
        stage('Packaging Stage') {
             steps {
                     sh "mvn package"
             }
        }
    }
}